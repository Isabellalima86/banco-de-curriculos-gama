            "application/json": {
              "schema": {
                "properties": {
                  "nome": {"type" : "string"},
                  "cargo": {"type" : "string"},
                  "nascimento": {"type" : "string"},
                  "civil": {"type" : "string"},
                  "sexo": {"type" : "string"},
                  "endereço": {"type": "string"},
                  "numero": {"type": "string"},
                  "bairro" : {"type": "string"},
                  "cidade": {"type" : "string"},
                  "estado": {"type" : "string"},
                  "cep": {"type" : "string"},
                  "telefone1": {"type" : "string"},
                  "telefone2": {"type" : "string"},
                  "celular": {"type" : "string"},
                  "contato": {"type" : "string"},
                  "email": {"type" : "string"},
                  "profissao": {"type" : "string"},
                  "rg": {"type" : "string"},
                  "cpf": {"type" : "string"},
                  "veiculo": {"type" : "string"},
                  "habilitacao": {"type" : "string"}
                }
              },
